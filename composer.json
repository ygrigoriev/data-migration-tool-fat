{
    "name": "magento/data-migration-tool-fat",
    "type": "library",
    "description": "Functional testing library for Magento Data Migration Tool",
    "require": {
        "php": "~5.6.0|7.0.2|~7.0.6",
        "magento/mtf": "1.0.0-rc43",
        "phpunit/phpunit": "4.1.0",
        "phpunit/phpunit-selenium": ">=1.2"
    },
    "suggest": {
        "netwing/selenium-server-standalone": "dev-master",
        "facebook/webdriver": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Magento\\DataMigrationTool\\Test\\": ["tests/app/Magento/DataMigrationTool/Test"],
            "Magento\\Mtf\\": ["/../../../vendor/magento/mtf/Magento/Mtf"],
            "Magento\\": ["/../../../dev/tests/functional/generated/Magento", "/../../../dev/tests/functional/tests/app/Magento"],
            "Test\\": "/../../../dev/tests/functional/generated/Test"
        }
    }
}
